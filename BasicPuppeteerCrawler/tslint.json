{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "printWidth": 240
      }
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["src/__tests__/*"]
  }
}
