{
  "name": "stolen-bike-cases",
  "version": "1.0.0",
  "description": "A system to report stolen bike cases and assign officer to resolved it",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node server.ts",
    "test": "nyc mocha --require ts-node/register test/**/*.ts"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "expressjs"
  ],
  "author": "Santoshkumar Yadav",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mysql": "^2.17.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/mysql": "^2.15.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 80,
    "functions": 80,
    "branches": 50,
    "lines": 80
  }
}
